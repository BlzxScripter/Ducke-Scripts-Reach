-- NOVAS FUN√á√ïES PARA ALCANCE M√ÅXIMO

function spawnMaxReachEffect()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if not character then return end
    
    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return end
    
    -- Efeito de explos√£o de alcance
    for i = 1, 20 do
        local part = Instance.new("Part")
        part.Size = Vector3.new(2, 2, 2)
        part.Position = root.Position + Vector3.new(
            math.random(-50, 50),
            math.random(0, 20),
            math.random(-50, 50)
        )
        part.Anchored = true
        part.CanCollide = false
        part.Transparency = 0.3
        part.BrickColor = BrickColor.new("Neon orange")
        part.Material = Enum.Material.Neon
        part.Parent = workspace
        
        game:GetService("Debris"):AddItem(part, 1)
    end
end

function createDetectionSphere(character, radius, color)
    local sphere = Instance.new("Part")
    sphere.Shape = Enum.PartType.Ball
    sphere.Size = Vector3.new(radius*2, radius*2, radius*2)
    sphere.Position = character:GetPivot().Position
    sphere.Anchored = false
    sphere.CanCollide = false
    sphere.Transparency = 0.95
    sphere.BrickColor = BrickColor.new(color)
    sphere.Material = Enum.Material.Neon
    sphere.Parent = character
    
    -- Manter esfera no personagem
    local weld = Instance.new("Weld")
    weld.Part0 = character:FindFirstChild("HumanoidRootPart") or character:WaitForChild("HumanoidRootPart")
    weld.Part1 = sphere
    weld.C0 = CFrame.new(0, 0, 0)
    weld.Parent = sphere
    
    return sphere
end

function attemptSuperKick(ball, target, distance)
    -- Tentar TODOS os eventos poss√≠veis
    local allEvents = {
        "KickBall", "KickEvent", "FootballKick", "Kick", "Shoot",
        "RemoteEvent", "RemoteFunction", "BallEvent", "GameEvent",
        "Replicate", "Replication", "ServerEvent", "ClientEvent"
    }
    
    for _, eventName in pairs(allEvents) do
        pcall(function()
            -- Tentar em diferentes locais
            local locations = {
                game:GetService("ReplicatedStorage"),
                game:GetService("Workspace"),
                game:GetService("ServerScriptService"),
                game:GetService("StarterPlayer")
            }
            
            for _, location in pairs(locations) do
                local remote = location:FindFirstChild(eventName)
                if remote and (remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction")) then
                    remote:FireServer(ball, target, distance)
                end
            end
        end)
    end
end

function attemptSuperSave(ball, savePoint)
    local saveEvents = {
        "GoalieSave", "SaveBall", "CatchBall", "Defend",
        "BlockBall", "Intercept", "GoalieEvent", "SaveEvent"
    }
    
    for _, eventName in pairs(saveEvents) do
        pcall(function()
            local remote = game:GetService("ReplicatedStorage"):FindFirstChild(eventName)
            if remote then
                remote:FireServer(ball, savePoint, _G.GoalieReach or 250)
            end
        end)
    end
end

function createSuperBeam(from, to, distance)
    -- Criar m√∫ltiplos beams para efeito visual
    for i = 1, math.min(10, math.floor(distance/10)) do
        task.spawn(function()
            local beam = Instance.new("Part")
            beam.Size = Vector3.new(0.3, 0.3, (from - to).Magnitude)
            beam.CFrame = CFrame.new(from, to) * CFrame.new(
                math.random(-2, 2),
                math.random(-2, 2),
                -beam.Size.Z/2
            )
            beam.Anchored = true
            beam.CanCollide = false
            beam.Transparency = 0.2
            beam.BrickColor = BrickColor.new("Bright yellow")
            beam.Material = Enum.Material.Neon
            beam.Parent = workspace
            
            game:GetService("Debris"):AddItem(beam, 0.3)
        end)
    end
end

function createQuickBeam(from, to)
    local beam = Instance.new("Part")
    beam.Size = Vector3.new(0.1, 0.1, (from - to).Magnitude)
    beam.CFrame = CFrame.new(from, to) * CFrame.new(0, 0, -beam.Size.Z/2)
    beam.Anchored = true
    beam.CanCollide = false
    beam.Transparency = 0.1
    beam.BrickColor = BrickColor.new("Lime green")
    beam.Parent = workspace
    
    game:GetService("Debris"):AddItem(beam, 0.1)
end

function createDefenseRing(center, radius)
    local ring = Instance.new("Part")
    ring.Shape = Enum.PartType.Cylinder
    ring.Size = Vector3.new(2, radius*2, radius*2)
    ring.Position = center
    ring.Anchored = true
    ring.CanCollide = false
    ring.Transparency = 0.7
    ring.BrickColor = BrickColor.new("Bright blue")
    ring.Material = Enum.Material.Neon
    ring.Parent = workspace
    
    game:GetService("Debris"):AddItem(ring, 0.5)
end

function calculateSaveDirection(goaliePos, ballPos)
    local direction = (ballPos - goaliePos).Unit
    -- Inverter dire√ß√£o para afastar do gol
    return -direction
end

function applyLocalForce(ball, direction)
    -- Aplicar for√ßa localmente (apenas visual)
    pcall(function()
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Velocity = (direction - ball.Position).Unit * 100
        bodyVelocity.MaxForce = Vector3.new(10000, 10000, 10000)
        bodyVelocity.Parent = ball
        
        game:GetService("Debris"):AddItem(bodyVelocity, 0.1)
    end)
end

function findNearestGoal(playerPos)
    -- Encontrar gols no campo
    local goals = {}
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj.Name:lower():find("goal") or obj.Name:lower():find("gol") then
            table.insert(goals, obj)
        end
    end
    
    if #goals > 0 then
        -- Retornar posi√ß√£o do gol mais pr√≥ximo
        local nearestGoal = goals[1]
        local nearestDist = (playerPos - nearestGoal.Position).Magnitude
        
        for i = 2, #goals do
            local dist = (playerPos - goals[i].Position).Magnitude
            if dist < nearestDist then
                nearestGoal = goals[i]
                nearestDist = dist
            end
        end
        
        return nearestGoal.Position
    end
    
    -- Se n√£o encontrar gol, chutar para frente
    return playerPos + Vector3.new(0, 0, 200)
end

-- Fun√ß√µes originais mantidas (com pequenas melhorias)
function findBallInWorkspace()
    local ballNames = {"SoccerBall", "Ball", "Football", "bola", "Bola", "Ball_Server", "ball", "BallInstance"}
    for _, name in pairs(ballNames) do
        local ball = workspace:FindFirstChild(name)
        if ball then 
            -- Procurar tamb√©m em subpastas
            return ball 
        end
    end
    
    -- Procurar em todos os descendentes
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and (obj.Name:lower():find("ball") or obj.Name:lower():find("bola")) then
            return obj
        end
    end
    
    return nil
end

