-- Configurar sistema para jogadores de campo (ATUALIZADO)
function setupFieldPlayerReach(reach)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    -- Sistema 1: Hitbox GIGANTE para alcance m√°ximo
    task.spawn(function()
        wait(0.5)
        
        local root = character:FindFirstChild("HumanoidRootPart")
        if not root then return end
        
        -- Criar hitbox invis√≠vel gigante ao redor do jogador
        if _G.MegaHitbox then _G.MegaHitbox:Destroy() end
        
        _G.MegaHitbox = Instance.new("Part")
        _G.MegaHitbox.Name = "MegaReachHitbox"
        _G.MegaHitbox.Size = Vector3.new(reach*2, reach*2, reach*2)
        _G.MegaHitbox.Position = root.Position
        _G.MegaHitbox.Anchored = false
        _G.MegaHitbox.CanCollide = false
        _G.MegaHitbox.Transparency = 1
        _G.MegaHitbox.Parent = character
        
        -- Conectar ao root part
        local weld = Instance.new("Weld")
        weld.Part0 = root
        weld.Part1 = _G.MegaHitbox
        weld.C0 = CFrame.new(0, 0, 0)
        weld.Parent = root
        
        -- Sistema de detec√ß√£o de bola em √°rea ampliada
        createDetectionSphere(character, reach, Color3.fromRGB(255, 50, 50))
    end)
    
    -- Sistema 2: Hook no mouse para chutes SUPER distantes
    local mouse = player:GetMouse()
    if _G.MouseConnection then _G.MouseConnection:Disconnect() end
    
    _G.MouseConnection = mouse.Button1Down:Connect(function()
        local ball = findBallInWorkspace()
        local root = character:FindFirstChild("HumanoidRootPart")
        
        if ball and root then
            local distance = (root.Position - ball.Position).Magnitude
            
            -- Se a bola estiver dentro do reach GIGANTE configurado
            if distance <= reach then
                -- Feedback visual melhorado
                createSuperBeam(root.Position, ball.Position, distance)
                
                -- Multiplos eventos para tentar bypass
                for i = 1, 5 do  -- Tentar 5 vezes
                    task.wait(0.01)
                    attemptSuperKick(ball, mouse.Hit.p, distance)
                end
                
                -- For√ßar movimento da bola localmente
                applyLocalForce(ball, mouse.Hit.p)
            end
        end
    end)
    
    -- Sistema 3: Chute autom√°tico quando bola perto (at√© 250 estudos)
    if _G.AutoKickThread then _G.AutoKickThread:Disconnect() end
    
    _G.AutoKickThread = game:GetService("RunService").Heartbeat:Connect(function()
        if _G.FieldPlayerReach and _G.FieldPlayerReach > 50 then  -- S√≥ se tiver alcance alto
            local ball = findBallInWorkspace()
            local root = character:FindFirstChild("HumanoidRootPart")
            
            if ball and root and _G.FieldPlayerReach then
                local distance = (root.Position - ball.Position).Magnitude
                
                -- Se bola estiver dentro do alcance E se movendo devagar
                if distance <= _G.FieldPlayerReach and ball.Velocity.Magnitude < 20 then
                    -- Direcionar para o gol mais pr√≥ximo
                    local goalTarget = findNearestGoal(root.Position)
                    attemptKickEvent(ball, goalTarget)
                    
                    -- Efeito visual
                    createQuickBeam(root.Position, ball.Position)
                end
            end
        end
    end)
end

-- Configurar sistema para goleiros (ATUALIZADO)
function setupGoalieReach(reach)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    -- Sistema para goleiros (CAMPO INTEIRO)
    task.spawn(function()
        wait(0.5)
        
        local root = character:FindFirstChild("HumanoidRootPart")
        if not root then return end
        
        -- Criar esfera de defesa GIGANTE
        if _G.GoalieSphere then _G.GoalieSphere:Destroy() end
        
        _G.GoalieSphere = Instance.new("Part")
        _G.GoalieSphere.Name = "GoalieMegaSphere"
        _G.GoalieSphere.Shape = Enum.PartType.Ball
        _G.GoalieSphere.Size = Vector3.new(reach*2, reach*2, reach*2)
        _G.GoalieSphere.Position = root.Position
        _G.GoalieSphere.Anchored = false
        _G.GoalieSphere.CanCollide = false
        _G.GoalieSphere.Transparency = 0.9
        _G.GoalieSphere.BrickColor = BrickColor.new("Bright blue")
        _G.GoalieSphere.Material = Enum.Material.Neon
        _G.GoalieSphere.Parent = character
        
        local weld = Instance.new("Weld")
        weld.Part0 = root
        weld.Part1 = _G.GoalieSphere
        weld.C0 = CFrame.new(0, 0, 0)
        weld.Parent = root
        
        -- Sistema de defesa de CAMPO INTEIRO
        createDetectionSphere(character, reach, Color3.fromRGB(50, 100, 255))
    end)
    
    -- Sistema de defesa SUPER autom√°tica para goleiros
    if _G.AutoDefenseThread then _G.AutoDefenseThread:Disconnect() end
    
    _G.AutoDefenseThread = game:GetService("RunService").Heartbeat:Connect(function()
        if _G.IsGoalieMode and _G.GoalieReach and _G.GoalieReach > 30 then
            local ball = findBallInWorkspace()
            local root = character:FindFirstChild("HumanoidRootPart")
            
            if ball and root then
                local distance = (root.Position - ball.Position).Magnitude
                
                -- Defender qualquer bola no alcance GIGANTE
                if distance <= _G.GoalieReach then
                    -- Teleportar bola para longe do gol
                    local saveDirection = calculateSaveDirection(root.Position, ball.Position)
                    local savePoint = root.Position + (saveDirection * 100)
                    
                    -- Multiplos eventos de defesa
                    for i = 1, 3 do
                        attemptSuperSave(ball, savePoint)
                    end
                    
                    -- Efeito visual de defesa
                    createDefenseRing(root.Position, math.min(distance, 100))
                end
            end
        end
    end)
end

